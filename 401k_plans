    <script type="text/javascript">
        // detect CSS display:flex support in JavaScript. Redirect if NOT supported
        var detector = document.createElement("detect");
        detector.style.display = "flex";
        if (detector.style.display.toLowerCase() === "flex") {
            console.log("Display value: CSS flex is supported");
        }
        else {
            detector.style.display = "-webkit-flex";
            if (detector.style.display.toLowerCase() === "-webkit-flex") {
                console.log("Display value: CSS -webkit-flex is supported");
            }
            else {
                console.log("Display value: CSS flexbox is not supported");
                self.location = "http://www.agiledesignlabs.com/noflex.php";
            }
            ;
        }
        ;
    </script>
    <script type="text/javascript">
        // detect browser versions that have poor support for flexbox and redirect
        // IE
        if (navigator.appName === "Microsoft Internet Explorer") {
            self.location = "http://www.agiledesignlabs.com/noflex.php";
        }
        ;
        // Firefox 27 & earlier
        if (navigator.userAgent.toLowerCase().indexOf("firefox") != -1) {
            ffSlicePos = navigator.userAgent.toLowerCase().indexOf("firefox") + 8;
            ffVersion = navigator.userAgent.toLowerCase().slice(ffSlicePos, ffSlicePos + 2);
            if (parseInt(ffVersion, 10) <= 27) {
                self.location = "http://www.agiledesignlabs.com/noflex.php";
            }
            ;
        }
        ;
    </script>
    <script type="text/javascript">
        var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
        document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
    </script>
